<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DefaultTextView">
        <attr name="TextStyle" format="enum">
            <enum name="regular" value="0" />
            <enum name="medium" value="1" />
            <enum name="semibold" value="2" />
            <enum name="bold" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="Button">
        <attr name="ButtonStyle" format="enum">
            <enum name="primary" value="0" />
            <enum name="secondary" value="1" />
            <enum name="outline" value="2" />
        </attr>
    </declare-styleable>
    <attr name="roColorInactive" format="color|reference" />
    <attr name="roColorBSB" format="color|reference" />
</resources>